@model DavideTrotta.AspNet.Redis.Web.Models.ResultViewModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body style="background-color:#008000">
    <div>
        <h1>Well... the time stamp in the session is always the same...</h1>
        <br />
        <b>Value in the session Variable</b>
        <p>@Model.ValueStoredInRedis</p>
    </div>
    <b>Is Caching in Redis: @TempData["IsCaching"]</b>
    <br />
    <br />
    @Html.ActionLink("Go to refresh page and check time stamp", "RefreshMe", "Home")
</body>
</html>
